<?xml version='1.0' encoding='utf-8'?>
<widget id="falckpf.dk" version="1.3.9" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" xmlns:android = "http://schemas.android.com/apk/res/android">
    <name>FalckPersonale</name>
    <content src="index.html" />
    <access origin="*" />
    <access origin="tel:*" launch-external="yes"/>
    <access origin="geo:*" launch-external="yes"/>
    <access origin="mailto:*" launch-external="yes"/>
    <access origin="sms:*" launch-external="yes"/>
    <access origin="market:*" launch-external="yes"/>
    
    <icon src="res/logo.png"/>
        <icon src="res/logo.png" density="ldpi" />
        <icon src="res/logo.png" density="mdpi" />
        <icon src="res/logo.png" density="hdpi" />
        <icon src="res/logo.png" density="xhdpi" />
        <icon src="res/logo.png" density="xxhdpi" />
        <icon src="res/logo.png" density="xxxhdpi" />
    
    <feature name="Calendar">
        <param name="android-package" value="nl.xservices.plugins.Calendar" />
    </feature>
    
    <plugin name="cordova-plugin-whitelist" source="npm" />
    <plugin name="cordova-plugin-calendar" source="npm" />
    
    <preference name="phonegap-version" value="cli-6.5.0" />
    <preference name="android-build-tool" value="gradle" />
    
    <meta http-equiv="Content-Security-Policy" content="default-src 'self' gap://ready file://* 'unsafe-inline' *; style-src 'self' 'unsafe-inline' *; script-src 'self' 'unsafe-inline' 'unsafe-eval' *; img-src *"/>
</widget>
